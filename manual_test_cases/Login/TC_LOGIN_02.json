{
    "TC_LOGIN_02": {
        "test_case_summary": "Verify that user cannot login with invalid password",
        "prerequisites": "1. Browser should be open\n2. User should exist",
        "steps": [
            {
                "test_step": "1. Navigate to the Login page",
                "expected_result": "Login page should be displayed",
                "test_data": {
                    "value": "https://rahulshettyacademy.com/client/#/auth/login"
                }
            },
            {
                "test_step": "2. Enter Valid Email",
                "expected_result": "Email should be entered",
                "test_data": {
                    "value": "valid_user@example.com"
                }
            },
            {
                "test_step": "3. Enter Invalid Password",
                "expected_result": "Password should be entered",
                "test_data": {
                    "value": "WrongPassword"
                }
            },
            {
                "test_step": "4. Click on 'Login' button",
                "expected_result": "Error message 'Incorrect email or password.' should be displayed",
                "test_data": {
                    "value": "none"
                }
            }
        ]
    }
}